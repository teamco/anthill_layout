{
  "name": "anthill",
  "private": true,
  "author": "Vladimir Tkach",
  "version": "1.0.2",
  "description": "SPA OOP Framework",
  "scripts": {
    "preinstall": "$(which node) -e \"if(process.env.npm_execpath.indexOf('yarn')===-1) throw new Error('You must use Yarn to install, not NPM')\"",
    "postinstall": "",
    "doc": "./node_modules/.bin/jsdoc -c jsdoc-conf.json",
    "clean": "$(which yarn) autoclean --force",
    "cache-clean": "$(which yarn) cache clean",
    "prebuild": "$(which yarn) run clean",
    "precompile": "$HOME/.rvm/gems/ruby-2.6.3/bin/rake assets:precompile",
    "compile": "$HOME/.rvm/gems/ruby-2.6.3/bin/bundle execa rails webpacker:compile",
    "build": "./node_modules/.bin/babel app/javascript/packs -d ./dist",
    "pretest": "",
    "jasmine-init": "$(which node) ./node_modules/jasmine/bin/jasmine.js init",
    "test": "./node_modules/.bin/babel-node ./node_modules/.bin/jasmine --random=false --config=test/unit/jasmine.json",
    "precoverage": "",
    "coverage": "./node_modules/.bin/babel-node ./node_modules/.bin/nyc npm test",
    "postcoverage": "",
    "rails-start": "$(which bundle) exec foreman start -f Procfile.dev",
    "build-dev": "webpack --mode development --config webpack.server.config.js && webpack --mode development --config webpack.dev.config.js --watch",
    "build-prod": "webpack --mode production --config webpack.server.config.js && webpack --mode production --config webpack.prod.config.js",
    "start-dev": "npm run build-dev && node ./dist/main.js --watch",
    "start-prod": "npm run build-prod && node ./dist/main.js"
  },
  "nyc": {
    "require": [
      "@babel/register"
    ]
  },
  "browserify-css": {
    "autoInject": true,
    "minify": true,
    "rootDir": "."
  },
  "postcss": {
    "parser": "postcss-scss",
    "map": false,
    "plugins": {
      "postcss-plugin": {},
      "autoprefixer": {},
      "postcss-preset-env": {}
    }
  },
  "dependencies": {
    "@babel/plugin-transform-destructuring": "^7.12.1",
    "@babel/plugin-transform-regenerator": "^7.12.1",
    "@babel/runtime": "^7.12.5",
    "@fortawesome/fontawesome-free": "^5.15.2",
    "@popperjs/core": "^2.6.0",
    "@rails/actioncable": "^6.1.1",
    "@rails/activestorage": "^6.1.1",
    "@rails/ujs": "^6.1.1",
    "@rails/webpacker": "^6.0.0-beta.4",
    "aggregation": "^1.2.7",
    "autoprefixer": "^10.2.4",
    "babel-plugin-macros": "^3.0.1",
    "bootstrap": "^4.6.0",
    "caller-id": "^0.1.0",
    "chart.js": "^2.9.4",
    "css-loader": "^5.0.1",
    "flag-icon-css": "^3.5.0",
    "gojs": "^2.1.34",
    "highlight.js": "^10.5.0",
    "html2canvas": "^1.0.0-rc.7",
    "jquery": "^3.5.1",
    "jquery-knob": "^1.2.11",
    "jquery-mousewheel": "^3.1.13",
    "jquery-simulate": "^1.0.2",
    "jquery-ui": "^1.12.1",
    "jquery.initialize": "^1.3.0",
    "jquery.nicescroll": "^3.7.6",
    "lz-string": "^1.4.4",
    "mics": "^0.7.0",
    "mini-css-extract-plugin": "^1.3.5",
    "module-alias": "^2.2.2",
    "moment": "^2.29.1",
    "npm": "^7.5.2",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "perfect-scrollbar": "^1.5.0",
    "pluralize": "^8.0.0",
    "popper.js": "^1.16.1",
    "raphael": "^2.3.0",
    "readability": "^0.1.0",
    "remove": "^0.1.5",
    "resolve-url-loader": "^3.1.2",
    "simple-line-icons": "^2.5.5",
    "tether": "^1.4.7",
    "through2": "^4.0.2",
    "tinymce": "^5.6.2",
    "to-string-loader": "^1.1.6",
    "turbolinks": "^5.2.0",
    "underscore": "^1.12.0",
    "underscore-deep-extend": "^1.1.5",
    "url-loader": "^4.1.1",
    "vkbeautify": "^0.99.3",
    "webpack": "^5.20.0",
    "webpack-cli": "^4.5.0"
  },
  "license": "MIT",
  "engines": {
    "node": ">=12.4.0",
    "yarn": ">=1.19.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
